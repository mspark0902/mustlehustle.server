{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "outDir": "./",
    "rootDir": "api",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "api/**/*.ts"
  ],
  "exclude": [
    "node_modules",
  ]
}